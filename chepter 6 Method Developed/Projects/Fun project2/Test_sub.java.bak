//implementing one value integer another value float
class Test{
	static void add(){
	int a=10;
	int b=20;
	int c=a+b;
		System.out.println("Addition Result:" + c);
	}
	static void sub(float a,short b){
	float c=a-b;
		System.out.println("Subtraction Result:" + c);
	}
}
class Test_sub{
	public static void main(String[] args){
		Test.add();
		Test.sub(20f,(short)10);
	}
}
